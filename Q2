// 2. Write a program that takes 5 integers as input from the user and print Max of all the numbers.
#include<stdio.h>
int main(){
    int array1[5];
    printf("Enter 5 numbers: \n");
    for (int i = 0; i <= 4; i++){
        scanf("%d",  &array1[i]);
    }
    int max = array1[0];
    for (int i = 1; i <= 4; i++){
        if (array1[i] > max)
        {
            max = array1[i];
        }
    }
    printf("Max of given numbers:  %d", max);
}
